   "source": [
    "// ▼ 元の配列\n",
    "const arr = [1, 2, 3, 4, 5];\n",
    "\n",
    "// ▼ 要素を順番に渡して関数を実行する。\n",
    "//    value: 値, index：要素番号, arr：配列オブジェクト自身\n",
    "arr.forEach((value, index, arr)=>{\n",
    "  console.log(`${index}:${value}`);\n",
    "});\n",
    "\n",
    "// ▼ 使用しない場合は引数を省略可能\n",
    "arr.forEach((value)=>{\n",
    "  console.log(`${value}`);\n",
    "});"
   ]
