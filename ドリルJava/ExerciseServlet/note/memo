USE myproject;

CREATE TABLE customer ( 
    id INTEGER PRIMARY KEY AUTO_INCREMENT, 
    login_id VARCHAR(100) NOT NULL UNIQUE, 
    password VARCHAR(100) NOT NULL 
);

